#parse("velocity/Layout.vm")
#@mainLayout()
<h1>Please Input Listing Details</h1>
<form action='/greeting_card'>
    <div class="form-group">
        <label for="address">Address</label>
        <input id="address" name="address" class="form-control" type="text">
    </div>
    <div class="form-group">
        <label for="rent">Rent Cost</label>
        <input id="rent" name="rent" class="form-control" type="text">
    </div>
    <div class="form-group">
        <label for="dis">Discription</label>
        <input id="dis" name="dis" class="form-control" type="text">
    </div>
    <button type="submit" class="btn">Go!</button>
</form>

#end